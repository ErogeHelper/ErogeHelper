<Window x:Class="ErogeHelper.View.HookConfigView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ErogeHelper.View"
        xmlns:ui="http://schemas.modernwpf.com/2019"
        xmlns:helper="clr-namespace:ErogeHelper.Common.Helper"
        xmlns:resx="clr-namespace:ErogeHelper.Language;assembly=ErogeHelper.Language"
        mc:Ignorable="d"
        ui:WindowHelper.UseModernWindowStyle="True" ResizeMode="CanResizeWithGrip"
        Width="350" MinWidth="350" Height="450" Icon="pack://application:,,,/Assets/app.ico"
        Title="{x:Static resx:Strings.HookConfig_Title}" helper:WindowPlacement.Save="True" Topmost="True">
    <Grid>
        <Frame x:Name="HookPage"/>
    </Grid>
</Window>
