<Button
    x:Class="ErogeHelper.View.MainGame.AssistiveTouch"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:maingame="clr-namespace:ErogeHelper.ViewModel.MainGame;assembly=ErogeHelper.ViewModel"
    mc:Ignorable="d"
    d:DataContext="{d:DesignInstance Type=maingame:AssistiveTouchViewModel}"
    HorizontalAlignment="Left" VerticalAlignment="Top"
    d:Template="{StaticResource NormalAssistiveTouchTemplate}"
    Click="AssistiveTouchOnClick" Focusable="False"
    Stylus.IsPressAndHoldEnabled="False" Stylus.IsTapFeedbackEnabled="False"
    Template="{DynamicResource AssistiveTouchStyle}"
    Visibility="Visible" />
