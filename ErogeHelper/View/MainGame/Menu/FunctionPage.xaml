<Page
    x:Class="ErogeHelper.View.MainGame.Menu.FunctionPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:comp="clr-namespace:ErogeHelper.Function.WpfComponent"
    xmlns:resx="clr-namespace:ErogeHelper.Common.Languages"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="300" d:Visibility="Visible"
    Visibility="Collapsed">

    <Grid x:Name="GridPanel">
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition />
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>

        <comp:TouchMenuItem x:Name="Back"
            Grid.Row="1" Grid.Column="1"
            Click="BackOnClick" Symbol="Back" Text=" " />

        <comp:ToggleTouchMenuItem x:Name="Attach"
            Grid.Row="1" Grid.Column="2"
            Symbol="Attach" Text="{x:Static resx:Strings.HookPage_Title}" />
        
    </Grid>
</Page>