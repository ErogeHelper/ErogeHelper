<Window x:Class="ErogeHelper.View.Dialogs.SavedataConflictDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ErogeHelper.View.Dialogs"
        xmlns:ui="http://schemas.modernwpf.com/2019"
        mc:Ignorable="d"
        Title="Warning" Height="250" Width="400"
        ui:WindowHelper.UseModernWindowStyle="True" Topmost="True"
        ShowInTaskbar="False" WindowStartupLocation="CenterOwner" ResizeMode="NoResize">
    <StackPanel>
        <TextBlock Text="Sync savedata from cloud immedtely, otherwise eh will upload file to cloud after game exit."/>
        <Button x:Name="Confirm" Content="Confirm" Click="Confirm_Click" />
        <Button x:Name="Cancel" Content="Cancel" Click="Cancel_Click" />
    </StackPanel>
</Window>
