<reactiveUi:ReactiveWindow 
    x:Class="ErogeHelper.View.Window.MainGameWindow"
    x:TypeArguments="vm:MainGameViewModel" 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://schemas.modernwpf.com/2019"
    xmlns:reactiveUi="http://reactiveui.net"
    xmlns:vm="clr-namespace:ErogeHelper.ViewModel.Window"
    ui:WindowHelper.UseModernWindowStyle="True"
    mc:Ignorable="d"
    WindowStartupLocation="CenterScreen"
    Title="MainWindow" 
    Height="450" 
    Width="800">
    <StackPanel>
        <TextBlock x:Name="ATextBlock" Style="{StaticResource BaseTextBlockStyle}" />
        <Button x:Name="AButton" Content="Button" />
        <TextBox x:Name="BindTextBox" />
    </StackPanel>
</reactiveUi:ReactiveWindow>