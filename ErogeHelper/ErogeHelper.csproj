<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <Nullable>enable</Nullable>
    <OutputPath>..\bin\$(Configuration)</OutputPath>
    <AppendTargetFrameworkToOutputPath>False</AppendTargetFrameworkToOutputPath>
    <ApplicationManifest>assets\app.manifest</ApplicationManifest>
    <Version>0.0.7</Version>
    <Authors>k1mlka</Authors>
    <MeCabUseDefaultDictionary>False</MeCabUseDefaultDictionary>
    <SatelliteResourceLanguages>zh-Hans</SatelliteResourceLanguages>
  </PropertyGroup>

  <ItemGroup>
	  <PackageReference Include="ModernWpfUis.MessageBox" Version="0.4.5" />
	  <PackageReference Include="Ookii.Dialogs.Wpf" Version="3.1.0" />
	  <PackageReference Include="ReactiveUI.Events.WPF" Version="13.2.18" />
	  <PackageReference Include="ReactiveUI.Fody" Version="13.2.18" />
	  <PackageReference Include="ReactiveUI.WPF" Version="13.2.18" />
  </ItemGroup>

</Project>