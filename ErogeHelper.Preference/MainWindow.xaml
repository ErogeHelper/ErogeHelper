<Window x:Class="ErogeHelper.Preference.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ErogeHelper.Preference"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Margin="20, 10">
        <CheckBox Name="OldScreenShot" Content="Alt+Enter instead of Win+Shift+S" Click="OldScreenShot_Click"  />
        <CheckBox Name="ZtwoEnter" Content="Z instead of Enter key" Margin="0 10 0 0" Click="ZtwoEnter_Click"  />
    </StackPanel>
</Window>
